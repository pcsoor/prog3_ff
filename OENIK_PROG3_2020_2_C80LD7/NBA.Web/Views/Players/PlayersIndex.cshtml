@model NBA.Web.Models.PlayerListViewModel

@{
    ViewData["Title"] = "PlayersIndex";
}

<h2>Players Index</h2>

<p style="color:red">@TempData["editResult"]</p>

<div style="display:flex">
    <div style="flex: 1">
        @await Html.PartialAsync("PlayersEdit", Model.EditedPlayer)
    </div>
    <div style="flex: 1">
        @await Html.PartialAsync("PlayersList", Model.ListOfPlayers)
    </div>
</div>



